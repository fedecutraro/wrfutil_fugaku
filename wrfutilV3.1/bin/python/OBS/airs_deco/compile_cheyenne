#!/bin/bash
##### 
## PAra compilar en Cheyenne
#####

F90='ifort'

FFLAGS='-assume underscore'

module unload netcdf
module load hdf-eos/2.20
module load hdf5/1.8.18
$F90  $FFLAGS -o decoder.airs dec_airsret_smn_rra_select_level.f90    -lhdfeos -lGctp  -lhdf5  -lmfhdf -ldf  -lz -lm -ljpeg  -lsz
module unload hdf5
module unload hdf-eos
module load netcdf
